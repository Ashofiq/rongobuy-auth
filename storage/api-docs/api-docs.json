{
    "openapi": "3.0.0",
    "info": {
        "title": "RongoBuy ecommerce",
        "version": "1.0"
    },
    "paths": {
        "/api/auth/v1/login": {
            "post": {
                "tags": [
                    "Auth"
                ],
                "summary": "RongoBuy login",
                "operationId": "69b4b7da857a88e2270a0a77507bd653",
                "requestBody": {
                    "description": "Pass user credentials",
                    "required": true,
                    "content": {
                        "application/json": {
                            "schema": {
                                "required": [
                                    "mobile",
                                    "password"
                                ],
                                "properties": {
                                    "mobile": {
                                        "type": "string",
                                        "example": "01767000000"
                                    },
                                    "password": {
                                        "type": "string",
                                        "format": "password",
                                        "example": "12345678"
                                    }
                                },
                                "type": "object"
                            }
                        }
                    }
                },
                "responses": {
                    "201": {
                        "description": "success"
                    }
                }
            }
        },
        "/api/auth/v1/registration": {
            "post": {
                "tags": [
                    "Auth"
                ],
                "summary": "RongoBuy Registration",
                "operationId": "deb365e13ee0cc9eb6d26769a377e94a",
                "requestBody": {
                    "description": "Pass user credentials",
                    "required": true,
                    "content": {
                        "application/json": {
                            "schema": {
                                "required": [
                                    "email",
                                    "password"
                                ],
                                "properties": {
                                    "name": {
                                        "type": "string",
                                        "example": "name"
                                    },
                                    "mobile": {
                                        "type": "string",
                                        "example": "01767000000"
                                    },
                                    "password": {
                                        "type": "string",
                                        "format": "password",
                                        "example": "12345678"
                                    },
                                    "type": {
                                        "type": "string"
                                    }
                                },
                                "type": "object"
                            }
                        }
                    }
                },
                "responses": {
                    "201": {
                        "description": "success"
                    }
                }
            }
        },
        "/api/v1/me": {
            "get": {
                "tags": [
                    "Auth"
                ],
                "summary": "RongoBuy User",
                "operationId": "356e7d100be5b721bce5b2f7d8dfdc77",
                "responses": {
                    "201": {
                        "description": "success"
                    }
                },
                "security": [
                    {
                        "bearer_token": []
                    }
                ]
            }
        }
    },
    "components": {
        "securitySchemes": {
            "bearer_token": {
                "type": "apiKey",
                "description": "Enter token in format (Bearer <token>)",
                "name": "Authorization",
                "in": "header"
            }
        }
    },
    "security": [
        []
    ]
}